{
  "name": "soltoken",
  "version": "1.0.0",
  "description": "A comprehensive toolkit for creating and managing custom tokens on Solana",
  "keywords": ["solana", "blockchain", "crypto", "token", "spl-token", "web3"],
  "author": "Your Name <your.email@example.com>",
  "homepage": "https://github.com/yourusername/soltoken",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/soltoken.git"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "mint": "node scripts/mint-token.js",
    "transfer": "node scripts/transfer-token.js",
    "balance": "node scripts/check-balance.js",
    "get-sol": "node scripts/get-sol.js",
    "disable-minting": "node scripts/disable-minting.js",
    "test": "jest",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "prepare": "npm run lint && npm test",
    "docs": "jsdoc -c jsdoc.json",
    "dev": "nodemon src/index.js"
  },
  "dependencies": {
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "^1.73.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "jsdoc": "^4.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4"
  }
}